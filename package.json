{
  "name": "webpack-multi-page-scaffold",
  "version": "0.4.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-cdn": "cross-env NODE_ENV=production CDN_ENV=1 webpack --config ./config/webpack.prod.js",
    "build-no-hash": "cross-env NODE_ENV=production NO_HASH_ENV=1 webpack --config ./config/webpack.prod.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "start": "webpack-dev-server --progress --config ./config/webpack.dev.js",
    "newpage": "node ./script/newpage.js",
    "preinstall": "node ./script/check-install-with-yarn.js",
    "lint": "eslint --fix src/**/*.{js,jsx}"
  },
  "keywords": [
    "webpack",
    "multiple",
    "pages"
  ],
  "author": "Fancy <fancyboynet@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.13",
    "install": "^0.11.0",
    "ip": "^1.1.5",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^2.1.5",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "standard",
      "standard-jsx"
    ]
  },
  "eslintIgnore": [
    "src/static/**/*.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "node ./script/check-package.js",
      "post-checkout": "node ./script/check-package.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
